            "dogs2_allowed_icon":"dog",
            "cats_allowed_icon":"cat",
            "floor_size_icon":"Fill 1",
            "pets_allowed_tag":"PET FRIENDLY",
            "dogs_allowed_tag":"DOG FRIENDLY",
            "cats_allowed_tag":"CAT FRIENDLY",
            "lease_length_tag":"Short stay"
        },
        "object": {
            "property_name":"building_name",
            "property_address":"address",
            "property_postal_code":"zipcode",
            "apartment_amenity": "amenity_tags",
            "building_amenity": "building_amenity_tags"
        },
        "attribute" : {
            "property_name":"name",
            "property_address":"streetAddress",
            "property_postal_code":"postalCode"
        }
    }
}