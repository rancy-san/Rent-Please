{
    "p": {
        "target_URL": "https://www.padmapper.com/apartments",
        "crawl_function": "webCrawl_p(this.browser, this.targetURL);",
        "rental_type": {
            "building": [
                "other",
                "condo",
                "house",
                "room",
                "apartment"
            ],
            "parameter": {
                "property_categories": "?property-categories=",
                "exclude_airbnb": "&exclude-airbnb"
            }
        },
        "selector": {
            "map": "MainMap_map",
            "map_button": "city_guide_showMapContainer",
