            "property_end": "list_noMoreResult",
            "property_backBtn": "btn btn-outline btn-back BubbleDetail_btnBack",
            "property_URL": "BubbleDetail_street",
            "property_container": "ListItemFull_listItemFull",
            "property_room_wrapper": "Floorplan_floorplanPanel",
            "property_room_container": "Floorplan_floorplanColumn",
            "property_room_type": "Floorplan_floorplanTitle",
            "property_room_type_check": "Floorplan_availabilityCount",
            "property_floor": "Floorplan_sqft",
            "property_bath": "Floorplan_bath",
            "property_cost": "Floorplan_floorplanPrice",
            "property_cost_default": "col BubbleDetail_colPrice",
            "property_basic_icon_default": "BubbleDetail_colSummaryIcon",
            "property_basic_tag_default": "PropertyBadge_propertyBadge",
            "property_amenity_container": "row p-no-gutter Amenities_amenities",
            "property_amenity": "Amenities_amenityContainer"
        },
